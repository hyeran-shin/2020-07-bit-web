		String id = request.getParameter("id");
		ServletContext sc = request.getServletContext();
		MemberDAO dao = (MemberDAO)sc.getAttribute("memberDTO");
		
		dao
		
		ModelAndView mav = new ModelAndView();
		return mav;
	}
}
